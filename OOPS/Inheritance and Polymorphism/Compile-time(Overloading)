class Addition{
    public int add(int a, int b){
        return a+b;

    }
    public int add(int a, int b, int c){
        return a+b+c;
    }
}
public class Main{
    public static void main(String[] args) {
        Addition addition = new Addition();
        System.out.println(addition.add(4, 5));
    }
}
