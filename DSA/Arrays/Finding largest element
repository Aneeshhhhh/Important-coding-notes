
public class Main{
    public static void main(String[]args){


        int[]arr={1, 2, 3, 4, 5, 6, 7};
        int x = Integer.MIN_VALUE; 


        for(int i = 0; i<arr.length;i++){
            if(arr[i]>x){
                x = arr[i];
            }





        }
        System.out.println(x);
    }

}
